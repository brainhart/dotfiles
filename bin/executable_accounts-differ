#!/usr/bin/env python3
import re
import sys


removal_regex = re.compile(r'\s*- "(.*)"')
addition_regex = re.compile(r'\s*\+ "(.*)"')

additions = []
removals = []

for line in sys.stdin:
    removal_search = removal_regex.search(line)
    if removal_search:
        removals.append(removal_search.group(1))

    addition_search = addition_regex.search(line)
    if addition_search:
        additions.append(addition_search.group(1))


true_additions = set(additions) - set(removals)
true_removals = set(removals) - set(additions)

for addition in true_additions:
    print(f"+ {addition}")

for removal in true_removals:
    print(f"- {removal}")
