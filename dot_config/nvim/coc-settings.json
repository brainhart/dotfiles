{
	"suggest.echodocSupport": true,
	"suggest.maxCompleteItemCount": 20,
	"html.filetypes": [
		"html",
		"handlebars"
	],
	"coc.preferences.formatOnSaveFiletypes": [
		"json",
	],
	"prettier.eslintIntegration": false,
	"python.formatting.provider": "black",
	"python.linting.flake8Enabled": false,
	"python.linting.pylintEnabled": false,
	"python.jediEnabled": false,
	"eslint.filetypes": [
		"javascript",
		"typescript",
		"typescriptreact",
		"javascriptreact"
	],
	"languageserver": {
		"bash": {
			"command": "bash-language-server",
			"args": [
				"start"
			],
			"filetypes": [
				"sh"
			],
			"ignoredRootPaths": [
				"~"
			]
		},
		"dockerfile": {
			"command": "docker-langserver",
			"filetypes": [
				"dockerfile"
			],
			"args": [
				"--stdio"
			]
		},
		"kotlin": {
			"command": "/Users/brian/code/source/kotlin-language-server/server/build/install/server/bin/kotlin-language-server",
			"filetypes": [
				"kotlin",
				"kt"
			],
			"rootPatterns": [
				".vim/",
				".git/",
				".hg/"
			]
		}
	}
}
